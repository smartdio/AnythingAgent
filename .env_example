# 应用配置
APP_NAME=AnythingAgent
DEBUG=true

# API配置
API_V1_STR=/v1

# 数据库配置
LANCEDB_URI=data/lancedb

# 安全配置
# 是否启用API密钥验证
ENABLE_API_KEY=false

# API密钥配置
API_KEY_NAME=Authorization
API_KEY_PREFIX=Bearer
# 在生产环境中应该使用更安全的密钥
API_KEY=dev-anything-agent-key

# 模型配置
DEFAULT_MODEL=gpt-3.5-turbo

# 文件配置
UPLOAD_DIR=data/uploads
# 10MB
MAX_FILE_SIZE=10485760

# 向量配置
# OpenAI的向量维度
VECTOR_SIZE=1536

# 服务器配置
HOST=0.0.0.0
PORT=8000
WORKERS=4

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

